<!doctype html>
<html>
<head>
    <meta charset="utf-8" />

    <title>{{#if page_meta}}{{page_meta.title}}{{else}}{{title}}{{/if}}</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">

    {{#if page_meta}}
    <meta name="description" content="{{page_meta.description}}">
    <meta name="keywords" content="{{page_meta.keywords}}">
    <meta name="robots" content="{{page_meta.robots}}">

    {{#if page_meta.canonical}}
    <link rel="canonical" href="{{page_meta.canonical}}" />
    {{/if}}

    <meta property="og:title" content="{{page_meta.title}}"/>
    <meta property="og:description" content="{{page_meta.description}}"/>
    <meta property="og:type" content="website"/>
    <meta property="og:url" content="https://view.ost.com"/>
    <meta property="og:site_name" content="view.ost.com"/>
    <meta property="og:image" content="{{page_meta.image}}"/>
    <meta name="twitter:card" content="summary_large_image"/>
    <meta name="twitter:title" content="{{page_meta.title}}"/>
    <meta name="twitter:description" content="{{page_meta.description}}"/>
    <meta name="twitter:image" content="{{page_meta.image}}"/>
    <meta name="google-site-verification" content="HXVSECDp1rMChIwEQ2pT9ti4NUHd7yAvXz8sQvvhyFw" />
    {{/if}}

    <link rel="icon" type="image/png" sizes="64x64" href="{{constants.cloud_front_base_domain}}/ost-kit/images/fav-icons/64x64.png">
    <link rel="apple-touch-icon" sizes="57x57" href="{{constants.cloud_front_base_domain}}/ost-kit/images/fav-icons/57x57.png">
    <link rel="apple-touch-icon" sizes="60x60" href="{{constants.cloud_front_base_domain}}/ost-kit/images/fav-icons/60x60.png">
    <link rel="apple-touch-icon" sizes="72x72" href="{{constants.cloud_front_base_domain}}/ost-kit/images/fav-icons/72x72.png">
    <link rel="apple-touch-icon" sizes="76x76" href="{{constants.cloud_front_base_domain}}/ost-kit/images/fav-icons/76x76.png">
    <link rel="apple-touch-icon" sizes="114x114" href="{{constants.cloud_front_base_domain}}/ost-kit/images/fav-icons/114x114.png">
    <link rel="apple-touch-icon" sizes="120x120" href="{{constants.cloud_front_base_domain}}/ost-kit/images/fav-icons/120x120.png">
    <link rel="apple-touch-icon" sizes="144x144" href="{{constants.cloud_front_base_domain}}/ost-kit/images/fav-icons/144x144.png">
    <link rel="apple-touch-icon" sizes="152x152" href="{{constants.cloud_front_base_domain}}/ost-kit/images/fav-icons/152x152.png">
    <link rel="apple-touch-icon" sizes="180x180" href="{{constants.cloud_front_base_domain}}/ost-kit/images/fav-icons/180x180.png">
    <link rel="icon" type="image/png" sizes="192x192" href="{{constants.cloud_front_base_domain}}/ost-kit/images/fav-icons/192x192.png">
    <link rel="icon" type="image/png" sizes="32x32" href="{{constants.cloud_front_base_domain}}/ost-kit/images/fav-icons/32x32.png">
    <link rel="icon" type="image/png" sizes="96x96" href="{{constants.cloud_front_base_domain}}/ost-kit/images/fav-icons/96x96.png">
    <link rel="icon" type="image/png" sizes="16x16" href="{{constants.cloud_front_base_domain}}/ost-kit/images/fav-icons/16x16.png">

    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">
    {{{css 'mCommon.css'}}}
    {{#each mCss}}
    {{{css this}}}
    {{/each}}
    <script>window.name = "{{randomStr}}";</script>

</head>
<body contenteditable="false">

    {{>topHeader}}
    {{{body}}}

    <script type="text/javascript" src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.12.9/umd/popper.min.js" integrity="sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q" crossorigin="anonymous"></script>
    <script type="text/javascript" src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/js/bootstrap.min.js" integrity="sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl" crossorigin="anonymous"></script>
    <script type="text/javascript" src="https://www.gstatic.com/charts/loader.js"></script>
    {{{js 'mCommon.js'}}}
    {{#each mJs}}
    {{{js this}}}
    {{/each}}
    {{{block "pageScripts"}}}

</body>
</html>
